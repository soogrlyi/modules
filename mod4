class Example {
    private String privateVar = "Private Variable";
    public String publicVar = "Public Variable";

    public String getPrivateVar() {
        return privateVar;
    }
}

class Employee {
    String name;
    double salary;

    public Employee(String name, double salary) {
        this.name = name;
        this.salary = salary;
    }

    public void displayInfo() {
        System.out.println("Name: " + name + ", Salary: " + salary);
    }
}

class Analyst extends Employee {
    String domain;

    public Analyst(String name, double salary, String domain) {
        super(name, salary);
        this.domain = domain;
    }

    @Override
    public void displayInfo() {
        super.displayInfo();
        System.out.println("Domain: " + domain);
    }
}

class Shape {
    private int numberOfSides;

    public Shape(int numberOfSides) {
        this.numberOfSides = numberOfSides;
    }

    public int getNumberOfSides() {
        return numberOfSides;
    }

    public void setNumberOfSides(int numberOfSides) {
        this.numberOfSides = numberOfSides;
    }
}

class Circle extends Shape {
    public Circle() {
        super(0);
    }
}

class Rectangle extends Shape {
    public Rectangle() {
        super(4);
    }
}

public class Main {
    public static void main(String[] args) {
        Example example = new Example();
        System.out.println(example.publicVar);
        System.out.println(example.getPrivateVar());

        Analyst analyst = new Analyst("John Doe", 60000, "Finance");
        analyst.displayInfo();

        Circle circle = new Circle();
        System.out.println("Circle sides: " + circle.getNumberOfSides());

        Rectangle rectangle = new Rectangle();
        System.out.println("Rectangle sides: " + rectangle.getNumberOfSides());
    }
}
